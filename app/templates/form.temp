<div class="form-wrapper">
    <form class="form fixed-by-width" ng-submit="onsubmit(content)" ng-class="content.class || ''">
        <div class="form-group" ng-repeat="item in content.items" ng-class="item.class || ''">
            <div attr-name="{{item.type}}" content="item.content"></div>
        </div>
    </form>
</div>